arousel.js","original":{"line":55,"column":15}}],"sources":{"Carousel.js":"import React from \"react\";\n\nclass Carousel extends React.Component{\n    constructor(props, context) {\n        super(props, context);\n        state = {\n            photos:[],\n            active: 0  //0 index\n        }\n    }\n    static getDerivedStateFromProps({media}) {\n        let photos = [];\n        if (media&& media.photos && media.photos.photo){\n            photos = media.photos.photo.filter(photo => photo[\"@size\"] === 'pn')\n        }\n        return { photos };\n    }\n\n    handleIndexClick (event){\n        this.setState({\n            active: +event.target.dataset.index\n        });\n    };\n\n    render () {\n        const { photos, active } = this.state;\n\n        let hero = \"http://placecorgi.com/300/300\";\n        if (photos[active] && photos[active].value) {\n            hero = photos[active].value;\n            }\n        return (\n            <div className=\"carousel\">\n                <img src={hero} alt=\"primary animal\" />\n                <div className=\"carousel-smaller\">\n                    {photos.map((photo, index) =>(\n                        // eslint-disable-next-line\n                       <img\n                       onClick={this.handleIndexClick}\n                       key={photo.value}\n                       data-index={index}\n                       src={photo.value}\n                       classNames={index === active ? \"active\": \"\"}\n                       //        IF  ..conditional THEN  else\n                       alt=\"animal thumbnail\"\n                    /> \n                    ))} \n                </div>\n            </div>\n        )\n    }\n    \n}\n\nexport default Carousel;"},"lineCount":null}},"error":null,"hash":"ac2cc32d80c302b96b0652336d6a8f91","cacheData":{"env":{}}}