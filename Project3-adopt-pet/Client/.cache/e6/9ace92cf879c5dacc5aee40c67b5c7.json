ser',\n      // `${process.env.REACT_APP_API_URL}/api/checkuser`, \n      { withCredentials:true }\n    )\n    .then(responseFromBackend => {\n      // console.log(\"Check User in APP.JS: \",responseFromBackend.data)\n      const { userDoc } = responseFromBackend.data;\n      this.syncCurrentUser(userDoc);\n    });\n  }\n  syncCurrentUser(user){\n    this.setState({ currentUser: user });\n    console.log(this.state);\n  }\n  logout(){\n    axios.delete(\n      `${process.env.REACT_APP_API_URL}/api/logout`,\n      {withCredentials:true}\n    )\n    .then(()=> this.syncCurrentUser(null))\n    .catch(err => console.log(err))\n  }\n\n  render() {\n    return (\n      <div>\n          <header>\n              <Link to='/'>Adopt Me!</Link>\n          </header>\n          <span>\n          <Link to='/signup'>Sign Up</Link>\n          <Link to='/login'>Login</Link>\n          </span>\n          {/* <Container> */}\n          \n            {/* <Login /> */}\n            {/* <ButtonExample /> */}\n          {/* </Container> */}\n          <Router>\n            <Switch >\n              {/* <Login path=\"/login\" /> */}\n              {/* <Route path=\"/login\" render={ () => \n                  <Login currentUser={ this.state.currentUser } \n                    onUserChange={userDoc => this.syncCurrentUser(userDoc)} />\n              } /> */}\n              <Route path=\"/login\"\n                render={() => <Login currentUser={ this.state.currentUser }\n                              onUserChange={userDoc => this.syncCurrentUser(userDoc)} />\n              }/>\n              \n              {/* <Signup path=\"/signup\" />\n              <Results path=\"/\" />\n              <Details path=\"/details/:id\"/> \n              <SearchParams path=\"/search-params\" />   */}\n            </Switch>\n          </Router>\n      </div>\n    );\n  }\n}\n\n\nReactDOM.render(<App />, document.getElementById(\"root\"));\n"},"lineCount":null}},"error":null,"hash":"86d0d985f5ee5cc027e8f156dab